/**
 * Cart cross sell items template
 *
 * @see Mage_Checkout_Block_Cart_Crosssell
 */
?>
<?php if($this->getItemCount()): ?>
	<ul>
    <?php foreach ($this->getItems() as $_item): ?>
        <li><a href="<?php echo $_item->getProductUrl() ?>" title="<?php echo $this->htmlEscape($_item->getName()) ?>"><img src="<?php echo $this->helper('catalog/image')->init($_item, 'thumbnail')->resize(114); ?>" width="114" height="11" alt="<?php echo $this->htmlEscape($_item->getName()) ?>" /><span class="numLikes"><img src="_images/icons/thumbs-up.png" alt="Thumbs Up icon"> 40</span></a></li>
    <?php endforeach; ?>
    </ul>
    
<?php endif; ?>